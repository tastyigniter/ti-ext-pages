{
    "name": "tastyigniter/ti-ext-pages",
    "type": "tastyigniter-package",
    "description": "Provides static pages and navigation menus for your TastyIgniter application.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Poyigi",
            "email": "sam@sampoyigi.com"
        }
    ],
    "require-dev": {
        "laravel/pint": "^1.2",
        "nunomaduro/larastan": "^2.4.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest-plugin-laravel": "^2.0",
        "tastyigniter/core": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Igniter\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Igniter\\Pages\\Tests\\": "tests/"
        }
    },
    "extra": {
        "tastyigniter-extension": {
            "code": "igniter.pages",
            "name": "Static Pages",
            "icon": {
                "class": "fa fa-files-o",
                "color": "#664500",
                "backgroundColor": "#FECC4D"
            },
            "homepage": "https://tastyigniter.com/marketplace/item/igniter-pages"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "static": "vendor/bin/phpstan analyse --ansi --memory-limit 1056M"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "composer/installers": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
